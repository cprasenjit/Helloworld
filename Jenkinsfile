pipeline {
	agent any
	stages {
		stage('Init') {
			steps {
				echo "this is Init stage"
			}			
		}
		stage('Build') {
			steps {
				echo "this is Build stage"
			}
		}
		stage('Deploy') {
			steps {
				echo "this is Deploy stage"
			}
		}
		stage('Deploy Production') {
			steps {
				echo "this is Deploy Production stage"
			}
		}
	}
}
